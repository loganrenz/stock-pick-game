{
  "name": "stock-pick-game",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "dev:vite": "vite",
    "dev:local": "vite --port 3004",
    "dev:vercel": "vercel dev",
