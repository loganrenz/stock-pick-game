{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "tsc",
        "outputDirectory": ".vercel/output/functions",
        "includeFiles": ["api/**/*.ts"]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/index.ts"
    },
    {
      "source": "/api/picks/:path*",
      "destination": "/api/picks/:path*/index.ts"
    },
    {
      "source": "/api/stocks/:path*",
      "destination": "/api/stocks/:path*/index.ts"
    },
    {
      "source": "/api/test/:path*",
      "destination": "/api/test/:path*/index.ts"
    },
    {
      "source": "/api/users/:path*",
      "destination": "/api/users/:path*/index.ts"
    },
    {
      "source": "/api/weeks/:path*",
      "destination": "/api/weeks/:path*/index.ts"
    }
  ],
